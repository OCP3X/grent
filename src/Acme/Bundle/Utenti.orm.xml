<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GRENT\Utenti" table="utenti">
    <id name="idUtente" type="string" column="id_utente" length="15">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descrizion" type="string" column="descrizion" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="password" type="string" column="password" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="dc" type="datetime" column="dc" nullable="true"/>
  </entity>
</doctrine-mapping>

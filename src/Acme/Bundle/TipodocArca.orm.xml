<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GRENT\TipodocArca" table="tipodoc_arca">
    <id name="idTipodoc" type="string" column="id_tipodoc" length="2">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descrizion" type="string" column="descrizion" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="tipoTraffico" type="string" column="tipo_traffico" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="idDitta" type="string" column="id_ditta" length="20" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

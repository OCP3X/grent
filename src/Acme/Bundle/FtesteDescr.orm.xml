<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GRENT\FtesteDescr" table="fteste_descr">
    <id name="idFtesta" type="integer" column="id_ftesta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="string" column="tipo" length="20" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="targa" type="string" column="targa" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="km" type="integer" column="km" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dataUscita" type="datetime" column="data_uscita" nullable="true"/>
    <field name="dataRientro" type="datetime" column="data_rientro" nullable="true"/>
    <field name="conducente" type="string" column="conducente" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="note" type="string" column="note" length="254" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

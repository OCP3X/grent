      <generator strategy="IDENTITY"/>
    </id>
    <field name="extNid" type="integer" column="ext_nid" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="extCid" type="string" column="ext_cid" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="sid" type="string" column="sid" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ordine" type="integer" column="ordine" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GRENT\Lavagna" table="lavagna">
    <id name="idL" type="integer" column="id_l">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idVeicolo" type="integer" column="id_veicolo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="anomalia" type="string" column="anomalia" length="400" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

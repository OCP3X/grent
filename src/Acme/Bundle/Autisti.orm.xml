<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GRENT\Autisti" table="autisti">
    <id name="idAutista" type="integer" column="id_autista">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cognome" type="string" column="cognome" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="nome" type="string" column="nome" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="dNascita" type="datetime" column="d_nascita" nullable="true"/>
    <field name="luogoNascita" type="string" column="luogo_nascita" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="patente" type="string" column="patente" length="20" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="dPatente" type="datetime" column="d_patente" nullable="true"/>
    <field name="dPatenteA" type="datetime" column="d_patente_a" nullable="true">
      <options>
        <option name="comment">data scadenza</option>
      </options>
    </field>
    <field name="indirizzo" type="string" column="indirizzo" length="100" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="citta" type="string" column="citta" length="50" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="tel" type="string" column="tel" length="30" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="luogo" type="string" column="luogo" length="30" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
